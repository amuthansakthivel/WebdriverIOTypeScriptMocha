{
  "name": "webdriveriomochaframework",
  "version": "1.0.0",
  "description": "WebdriverIO v7 UI Automation Framework",
  "main": "index.js",
  "scripts": {
    "test": "npx wdio src/web/config/wdio.conf.ts",
    "multi-browser": "npx wdio src/web/config/wdio.conf.cross-browser.ts",
    "smoke": "npx wdio wdio.conf.ts --suite smoke",
    "api": "mocha --timeout 20000 --reporter mochawesome --reporter-options reportDir=reports/api -r tsconfig-paths/register -r ts-node/register src/api/test/*.ts",
    "report": "marge ./reports/json/wdio-ma-merged.json --reportTitle UIAutomationReport --reportDir=reports/mocha && move .\\reports\\mocha.html .\\reports\\mocha",
    "cleanup": "rmdir /Q /S reports"
  },
  "author": "MD SADAB SAQIB",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/faker": "^5.5.3",
    "@types/supertest": "^2.0.11",
    "@wdio/allure-reporter": "^7.16.14",
    "@wdio/cli": "^7.16.15",
    "@wdio/local-runner": "^7.16.15",
    "@wdio/mocha-framework": "^7.16.15",
    "@wdio/selenium-standalone-service": "^7.16.14",
    "@wdio/spec-reporter": "^7.16.14",
    "allure-commandline": "^2.17.2",
    "chai": "^4.3.6",
    "faker": "^5.5.3",
    "mochawesome": "^7.0.1",
    "mochawesome-report-generator": "^3.1.5",
    "supertest": "^6.2.2",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "wdio-mochawesome-reporter": "^4.0.0"
  }
}
