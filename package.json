{
  "name": "webdriveriomochaframework",
  "version": "1.0.0",
  "description": "WebdriverIO v7 UI Automation Framework",
  "main": "index.js",
  "scripts": {
    "test": "npx wdio wdio.conf.ts",
    "uitest": "npx wdio wdio.conf.ts --suite uiTests",
    "api": "mocha --timeout 20000 --reporter mochawesome --reporter-options reportDir=reports/api -r tsconfig-paths/register -r ts-node/register src/api/test/*.ts",
    "report": "marge ./reports/json/wdio-ma-merged.json && move mochawesome-report.html ./mochawesome-report"
  },
  "author": "MD SADAB SAQIB",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/faker": "^5.5.9",
    "@types/supertest": "^2.0.11",
    "@wdio/allure-reporter": "^7.16.8",
    "@wdio/cli": "^7.16.9",
    "@wdio/local-runner": "^7.16.9",
    "@wdio/mocha-framework": "^7.16.6",
    "@wdio/selenium-standalone-service": "^7.16.6",
    "@wdio/spec-reporter": "^7.16.9",
    "@wdio/sync": "^7.16.9",
    "allure-commandline": "^2.13.8",
    "chai": "^4.3.4",
    "faker": "^5.5.3",
    "mochawesome": "^7.0.1",
    "mochawesome-report-generator": "^3.1.5",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "wdio-mochawesome-reporter": "^4.0.0",
    "webdriverio": "^7.16.9"
  }
}
