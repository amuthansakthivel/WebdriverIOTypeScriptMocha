{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonJs",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@Pages/*": [
        "src/pages/*"
      ],
      "@Static/*": [
        "src/static/*"
      ],
      "@Resources/*": [
        "src/resources/*"
      ]
    },
    "types": [
      "node",
      "webdriverio/async",
      "@wdio/mocha-framework",
      "@wdio/selenium-standalone-service",
      "expect-webdriverio"
    ]
  }
}