{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonJs",
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@UIPages/*": [
        "src/web/pages/*"
      ],
      "@UIStatic/*": [
        "src/web/static/*"
      ],
      "@API/*": [
        "src/api/*"
      ]
    },
    "types": [
      "node",
      "webdriverio/async",
      "@wdio/mocha-framework",
      "@wdio/selenium-standalone-service",
      "expect-webdriverio"
    ]
  }
}